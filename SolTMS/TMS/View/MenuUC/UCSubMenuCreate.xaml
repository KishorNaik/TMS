<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="TMS.View.MenuUC.UCSubMenuCreate"
	x:Name="UserControl"
	d:DesignWidth="320" d:DesignHeight="400">

	<Grid x:Name="LayoutRoot">
		<StackPanel Grid.Row="0" Grid.Column="0" Style="{StaticResource SubMenuStackPanelStyle}" ScrollViewer.VerticalScrollBarVisibility="Auto">
			<Button Content="Consignment" Style="{StaticResource SubMenuButtonStyle}"/>
			<Button Content="Dispatch" Style="{StaticResource SubMenuButtonStyle}"/>
			<Button Content="POD" Style="{StaticResource SubMenuButtonStyle}"/>
		</StackPanel>
	</Grid>
</UserControl>